@model LatiumMarketplace.Models.BidViewModels.Bid


@{
    ViewData["Title"] = "My Bids";
}


<head>
    <link href="~/css/bids.css" rel="stylesheet" />
</head>

<h2>My Bids</h2>


<div id="exTab1" class="container">
    <ul class="nav nav-pills">
        <li class="active">
            <a href="#1a" data-toggle="tab">Inbox</a>
        </li>
        <li>
            <a href="#2a" data-toggle="tab">Outbox</a>
        </li>
     
    </ul>

    <div class="tab-content clearfix">
        <div class="tab-pane active" id="1a">
            <h3>Here are the Bids placed on your assets</h3>
        </div>
        <div class="tab-pane" id="2a">
            <h3>Here are the bids you placed on other peoples assests</h3>
            <table class="table">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(m => m.asset_name)
                        </th>
                        <td>
                            @Html.DisplayFor(m => m.asset_name)
                        </td>
                        <th>
                            @Html.DisplayNameFor(m => m.bidPrice)
                        </th>
                        <td>
                            @Html.DisplayFor(m => m.bidPrice)
                        </td>

                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    <!---foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                Html.DisplayFor(modelItem => item.asset.name)
                            </td>
                  
                        </tr>
                    }-->
                </tbody>
            </table>

        </div>
    </div>
</div>



